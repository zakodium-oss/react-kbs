{
  "name": "react-kbs",
  "version": "0.2.0",
  "description": "React library to manage your application's keyboard shortcuts",
  "homepage": ".",
  "main": "./lib-cjs/index.js",
  "module": "./lib-esm/index.js",
  "types": "./lib-esm/index.d.ts",
  "files": [
    "lib-cjs",
    "lib-esm"
  ],
  "scripts": {
    "build": "vite build",
    "check-types": "tsc --noEmit",
    "compile": "npm run compile-cjs && npm run compile-esm",
    "compile-cjs": "tsc --project tsconfig.cjs.json",
    "compile-esm": "tsc --project tsconfig.esm.json",
    "dev": "vite",
    "eslint": "eslint .",
    "eslint-fix": "npm run eslint -- --fix",
    "prepack": "npm run compile",
    "prettier": "prettier --check .",
    "prettier-write": "prettier --write .",
    "serve": "vite preview",
    "test": "npm run eslint && npm run prettier && npm run check-types"
  },
  "keywords": [
    "react",
    "keyboard",
    "shortcuts",
    "shortcut"
  ],
  "license": "MIT",
  "volta": {
    "node": "16.4.2"
  },
  "devDependencies": {
    "@headlessui/react": "^1.3.0",
    "@heroicons/react": "^1.0.2",
    "@types/node": "^16.3.1",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.1.8",
    "@vitejs/plugin-react-refresh": "^1.3.5",
    "@zakodium/eslint-config": "^3.0.2",
    "autoprefixer": "^10.3.0",
    "clsx": "^1.1.1",
    "eslint": "^7.30.0",
    "postcss": "^8.3.5",
    "prettier": "^2.3.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "tailwindcss": "^2.2.4",
    "typescript": "^4.3.5",
    "vite": "^2.4.1"
  }
}
