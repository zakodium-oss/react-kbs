{
  "name": "react-kbs",
  "version": "1.0.0",
  "description": "React library to manage your application's keyboard shortcuts",
  "homepage": ".",
  "main": "./lib-cjs/index.js",
  "module": "./lib-esm/index.js",
  "types": "./lib-esm/index.d.ts",
  "files": [
    "lib-cjs",
    "lib-esm"
  ],
  "scripts": {
    "build": "vite build",
    "check-types": "tsc --noEmit",
    "compile": "npm run compile-cjs && npm run compile-esm",
    "compile-cjs": "tsc --project tsconfig.cjs.json",
    "compile-esm": "tsc --project tsconfig.esm.json",
    "dev": "vite",
    "eslint": "eslint .",
    "eslint-fix": "npm run eslint -- --fix",
    "prepack": "npm run compile",
    "prettier": "prettier --check .",
    "prettier-write": "prettier --write .",
    "serve": "vite preview",
    "test": "npm run eslint && npm run prettier && npm run check-types"
  },
  "keywords": [
    "react",
    "keyboard",
    "shortcuts",
    "shortcut"
  ],
  "license": "MIT",
  "volta": {
    "node": "16.4.2"
  },
  "devDependencies": {
    "@headlessui/react": "^1.6.1",
    "@heroicons/react": "^1.0.6",
    "@types/node": "^17.0.31",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.3",
    "@vitejs/plugin-react": "^1.3.2",
    "@zakodium/eslint-config": "^5.1.1",
    "autoprefixer": "^10.4.7",
    "clsx": "^1.1.1",
    "eslint": "^8.14.0",
    "postcss": "^8.4.13",
    "prettier": "^2.6.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-router-dom": "^6.3.0",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.6.4",
    "vite": "^2.9.7"
  }
}
