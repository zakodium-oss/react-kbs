{
  "name": "react-kbs",
  "version": "2.1.0",
  "description": "React library to manage your application's keyboard shortcuts",
  "homepage": ".",
  "main": "./lib-cjs/index.js",
  "module": "./lib-esm/index.js",
  "types": "./lib-esm/index.d.ts",
  "files": [
    "lib-cjs",
    "lib-esm"
  ],
  "scripts": {
    "build": "vite build",
    "check-types": "tsc --noEmit",
    "compile": "npm run compile-cjs && npm run compile-esm",
    "compile-cjs": "tsc --project tsconfig.cjs.json",
    "compile-esm": "tsc --project tsconfig.esm.json",
    "dev": "vite",
    "eslint": "eslint .",
    "eslint-fix": "npm run eslint -- --fix",
    "prepack": "npm run compile",
    "prettier": "prettier --check .",
    "prettier-write": "prettier --write .",
    "serve": "vite preview",
    "test": "npm run eslint && npm run prettier && npm run check-types"
  },
  "keywords": [
    "react",
    "keyboard",
    "shortcuts",
    "shortcut"
  ],
  "license": "MIT",
  "volta": {
    "node": "16.18.0"
  },
  "devDependencies": {
    "@headlessui/react": "^1.7.3",
    "@heroicons/react": "^2.0.12",
    "@types/node": "^18.11.7",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@vitejs/plugin-react": "^2.2.0",
    "@zakodium/eslint-config": "^6.0.0",
    "autoprefixer": "^10.4.13",
    "clsx": "^1.2.1",
    "eslint": "^8.26.0",
    "postcss": "^8.4.18",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.2",
    "tailwindcss": "^3.2.1",
    "typescript": "^4.8.4",
    "vite": "^3.2.1"
  }
}
