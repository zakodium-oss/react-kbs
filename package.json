{
  "name": "react-kbs",
  "version": "2.1.1",
  "description": "React library to manage your application's keyboard shortcuts",
  "homepage": ".",
  "main": "./lib-cjs/index.js",
  "module": "./lib-esm/index.js",
  "types": "./lib-esm/index.d.ts",
  "files": [
    "lib-cjs",
    "lib-esm"
  ],
  "scripts": {
    "build": "vite build",
    "check-types": "tsc --noEmit",
    "compile": "npm run compile-cjs && npm run compile-esm",
    "compile-cjs": "tsc --project tsconfig.cjs.json",
    "compile-esm": "tsc --project tsconfig.esm.json",
    "dev": "vite",
    "eslint": "eslint .",
    "eslint-fix": "npm run eslint -- --fix",
    "prepack": "npm run compile",
    "prettier": "prettier --check .",
    "prettier-write": "prettier --write .",
    "serve": "vite preview",
    "test": "npm run eslint && npm run prettier && npm run check-types"
  },
  "keywords": [
    "react",
    "keyboard",
    "shortcuts",
    "shortcut"
  ],
  "license": "MIT",
  "volta": {
    "node": "20.16.0"
  },
  "devDependencies": {
    "@headlessui/react": "^2.1.2",
    "@heroicons/react": "^2.1.5",
    "@types/node": "^22.1.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "clsx": "^2.1.1",
    "eslint": "^8.33.0",
    "eslint-config-zakodium": "^12.0.0",
    "postcss": "^8.4.41",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.0",
    "tailwindcss": "^3.4.7",
    "typescript": "^5.5.4",
    "vite": "^5.3.5"
  }
}
